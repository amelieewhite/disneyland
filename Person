public class Person
{
  private String name;
  private int energy;
  private int hunger;
  private int morale;
  public Person(String n, int e, int h, int m)
  {
    name = n;
    energy = e;
    hunger = h;
    morale = m;
  }
  public void incEnergy(int amount)
  {
    energy += amount;
  }
  public void decEnergy(int amount)
  {
    energy -= amount;
  }
}
